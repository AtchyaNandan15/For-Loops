    cin>> r; 
    
    int i;
    
    for (i=1;i<=1;i++)
    
    {
     cout<<" **********\n";
    }

 for (i=1;i<=1;i++) {
     cout<<" *********\n ";
     
 }
 
  for (i=1;i<=1;i++) {
     cout<<" ********\n ";
     
 }
 
 
  for (i=1;i<=1;i++) {
     cout<<" *******\n ";
     
 }
 
 
  for (i=1;i<=1;i++) {
     cout<<" ******\n ";
     
 }
 
 
 
  for (i=1;i<=1;i++) {
     cout<<" *****\n ";
     
 }
 
 
  for (i=1;i<=1;i++) {
     cout<<" ****\n ";
     
 }
 

 
 
  for (i=1;i<=1;i++) {
     cout<<" ***\n ";
     
 }
 
 
 
  for (i=1;i<=1;i++) {
     cout<<" **\n";
     
 }
 
 
 
  for (i=1;i<=1;i++) {
     cout<<" * ";
     
 }
 
 
    
    
}
